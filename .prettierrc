{
    "prettier.configPath": {
        "plugins": ["prettier-plugin-tailwindcss"],
        "tailwindStylesheet": "./src/style.css",
        "semi": true,
        "tabWidth": 4,
        "printWidth": 100,
        "singleQuote": false,
        "trailingComma": "es5",
        "bracketSpacing": true,
        "arrowParens": "always",
        "vueIndentScriptAndStyle": true,
        "endOfLine": "auto"
    },
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "[vue]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    }
}
